[alias]
  st = status
  ci = commit
  co = checkout
  br = branch
  ca = commit -av
  mg = merge --no-ff
  lo = log --graph
  undo = reset --hard
  lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
  amend = commit --amend
  unstage = rm --cached
  tinylog = log --format=oneline --abbrev-commit
